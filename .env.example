# Your EmailJS Service ID
REACT_APP_EMAILJS_SERVICE_ID=your_service_id_here

# Template IDs for different forms (can use same template for all forms)
REACT_APP_EMAILJS_TEMPLATE_ID_CONTACT=your_template_id_here
